{
  "name": "nlw-omnistack-server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "start:dev": "nodemon --ignore node_modules--watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/server.ts' -e ts",
    "husky:prepare": "husky install",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:push": "yarn test --coverage",
    "knex:migrate": "knex migrate:latest --knexfile knexfile.ts",
    "knex:seed": "knex seed:run --knexfile knexfile.ts",
    "postinstall": "yarn husky:prepare"
  },
  "author": "Â¡Duque",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "git-commit-msg-linter": "^4.5.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "knex": "^2.4.0",
    "sqlite3": "^5.1.4"
  }
}
